{
  "rewrites": [
    {
      "source": "/pl/integracje/kategoria/:category/",
      "destination": "/_render"
    },
    {
      "source": "/pl/integracje/kategoria/:category/strona/:page/",
      "destination": "/_render"
    },
    {
      "source": "/pl/hotele/:hotel/",
      "destination": "/_render"
    },
    {
      "source": "/pl/realizacje/:slug/",
      "destination": "/_render"
    },
    {
      "source": "/pl/blog/:slug/",
      "destination": "/_render"
    }
  ],
  "headers": [
    {
      "source": "/pl/hotele/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, max-age=0"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-store"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source": "/pl/integracje/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, max-age=0"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-store"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
