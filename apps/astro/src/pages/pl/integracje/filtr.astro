---
export const prerender = false

import ActivitiesListing, { fetchData } from '@/src/templates/activities/CategoriesPage.astro'

const data = await fetchData('pl', 1, Astro.url.origin, Astro.url.searchParams)

// Calculate canonical URL (remove /filtr from current path)
const canonicalUrl = Astro.url.origin + '/pl/integracje/'

if (!data) return Astro.rewrite('/pl/404')
---

<ActivitiesListing {...data} currentPage={1} canonicalUrl={canonicalUrl} />
