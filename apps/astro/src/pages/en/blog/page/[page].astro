---
import BlogPage, { fetchData, staticPathsPage } from '@/src/templates/blog/BlogPage.astro'
const category = null
const page = Number(Astro.params.page) || 1
const data = await fetchData('en', category, page)

if (!data) return Astro.rewrite('/en/404')

export const getStaticPaths = () => staticPathsPage('en')
---

<BlogPage {...data} currentPage={page} currentCategory={null} />

