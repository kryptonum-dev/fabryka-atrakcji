---
import BlogPostPage, { fetchData, staticPaths } from '@/src/templates/blog/SingleBlogPost.astro'

const slug = Astro.params.slug || ''
const data = await fetchData(slug, 'en')
if (!data) return Astro.rewrite('/en/404')

export const getStaticPaths = () => staticPaths('en')
---

<BlogPostPage {...data} lang="en" slug={slug} />

